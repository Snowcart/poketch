{
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react",
		"lib": ["es2015", "es2017", "dom"],
		"module": "es6",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./wwwroot/dist/",
		"paths": {
			"react": ["node_modules/react"]
		},
		"sourceMap": true,
		"target": "es6"
	},
	"exclude": ["node_modules", "typings", "wwwroot"]
}
