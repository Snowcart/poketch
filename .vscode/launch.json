{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "dev",
      "program": "${workspaceFolder}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "args": ["--config", "webpack.development.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "dev-fast",
      "program": "${workspaceFolder}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "args": ["--config", "webpack.development-fast.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "dev-fast-debug",
      "program": "${workspaceFolder}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "args": ["--config", "webpack.development-fast.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "autoAttachChildProcesses": true
    }
  ]
}
